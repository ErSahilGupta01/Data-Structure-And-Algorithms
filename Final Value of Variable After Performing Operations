class Solution {
public:
    int finalValueAfterOperations(vector<string>& v) 
    {   int x=0;
       for(int i=0;i<v.size();i++)
       {
           if(v[i]=="++X"||v[i]=="X++")
           {
               x+=1;
           }
           else 
           {
               x-=1;
           }
       }
     return x;
    }
};
